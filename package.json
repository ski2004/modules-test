{
  "name": "js-f2e",
  "version": "0.3.12",
  "author": "jsF2E",
  "main": "dist/vue-F2E.common.js",
  "files": [
    "dist",
    "lib"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "npm run build:wc && npm run build:cp:lib && npm run build:js && npm run build:css && npm run build:cp:view",
    "build:win": "npm run build:wc:win && xcopy /E/Y .\\dist .\\lib && npm run build:js && npm run build:css && xcopy /E/Y .\\src\\components\\* .\\dist\\components\\*",
    "build:cp:lib": "cp -r ./dist/ ./lib/",
    "build:cp:view": "cp -r ./src/components/ ./dist/components/ ",
    "build:js": "vue-cli-service build --target lib --name vue-F2E ./src/components/index.js",
    "build:wc": "vue-cli-service build --target wc-async --name f2e './src/components/*/*.vue'",
    "build:wc:win": "vue-cli-service build --target wc-async --name f2e ./src/components/*/*.vue",
    "build:css": "node-sass src/assets/scss/ -o dist/css/",
    "build:doc": "vue-cli-service build ./src/main.js --dest docs --mode development",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e"
  },
  "dependencies": {
    "bootstrap": "^4.5.0",
    "core-js": "^3.6.5",
    "jquery": "^3.5.1",
    "lodash": "^4.17.11",
    "popper.js": "^1.16.1",
    "vue": "^2.6.11",
    "vue-router": "^3.2.0",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.4.0",
    "@vue/cli-plugin-e2e-nightwatch": "^4.4.0",
    "@vue/cli-plugin-router": "^4.4.0",
    "@vue/cli-plugin-unit-mocha": "^4.4.0",
    "@vue/cli-plugin-vuex": "^4.4.0",
    "@vue/cli-service": "^4.4.0",
    "@vue/test-utils": "^1.0.3",
    "chai": "^4.1.2",
    "chromedriver": "^80.0.0",
    "geckodriver": "^1.19.1",
    "node-sass": "^4.14.1",
    "sass-loader": "^8.0.2",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "vue-template-compiler": "^2.6.11"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
